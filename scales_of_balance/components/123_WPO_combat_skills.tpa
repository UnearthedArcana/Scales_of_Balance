/*
APPEND ~splprot.2da~ ~D5_LEVEL1_IS%TAB%34%TAB%-1%TAB%1~ UNLESS ~D5_LEVEL1_IS~
APPEND ~splprot.2da~ ~D5_LEVEL1_MORE%TAB%34%TAB%-1%TAB%3~ UNLESS ~D5_LEVEL1_MORE~
APPEND ~splprot.2da~ ~D5_LEVEL2_IS%TAB%34%TAB%-1%TAB%1~ UNLESS ~D5_LEVEL2_IS~
APPEND ~splprot.2da~ ~D5_LEVEL2_MORE%TAB%34%TAB%-1%TAB%3~ UNLESS ~D5_LEVEL2_MORE~
COPY_EXISTING ~splprot.2da~ ~override~
	COUNT_2DA_COLS cols
	READ_2DA_ENTRIES_NOW rows cols
	FOR (row = 1; row < rows; ++row) BEGIN
	  READ_2DA_ENTRY_FORMER rows row 0 ~stat~
	  PATCH_IF (~%stat%~ STRING_EQUAL_CASE ~D5_LEVEL1_IS~) BEGIN
	    SET level1_is_row = %row%
	  END
	  PATCH_IF (~%stat%~ STRING_EQUAL_CASE ~D5_LEVEL1_MORE~) BEGIN
	    SET level1_more_row = %row%
	  END
	  PATCH_IF (~%stat%~ STRING_EQUAL_CASE ~D5_LEVEL2_IS~) BEGIN
	    SET level2_is_row = %row%
	  END
	  PATCH_IF (~%stat%~ STRING_EQUAL_CASE ~D5_LEVEL2_MORE~) BEGIN
	    SET level2_more_row = %row%
	  END
	END
BUT_ONLY

