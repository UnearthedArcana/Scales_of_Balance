//combine weapons to make room for CSPs______________________________________________
// if CDTweaks BG1 or IWD prof system installed, switch their profs to make room for CSP

ACTION_IF (MOD_IS_INSTALLED ~setup-cdtweaks.tp2~ ~2161~) OR (MOD_IS_INSTALLED ~setup-cdtweaks.tp2~ ~2162~) OR (MOD_IS_INSTALLED ~setup-cdtweaks.tp2~ ~2163~) OR (MOD_IS_INSTALLED ~setup-cdtweaks.tp2~ ~2164~) BEGIN
  // - move 107 to 89
  // - move 106 to 90
  // - move 105 to 91
  // - move 104 to 92

//update weapprof____________________________________________________________________
//
 COPY_EXISTING ~weapprof.2da~ ~override~
  COUNT_2DA_COLS cols
  COUNT_2DA_ROWS cols rows
  FOR (row = 7; row < rows; ++row) BEGIN
    READ_2DA_ENTRY row 1 cols stat
    PATCH_IF (~%stat%~ STRING_EQUAL_CASE ~104~) BEGIN
      READ_2DA_ENTRY row 0 cols 104_label
      READ_2DA_ENTRY row 2 cols 104_name
      READ_2DA_ENTRY row 3 cols 104_desc
    END
    PATCH_IF (~%stat%~ STRING_EQUAL_CASE ~105~) BEGIN
      READ_2DA_ENTRY row 0 cols 105_label
      READ_2DA_ENTRY row 2 cols 105_name
      READ_2DA_ENTRY row 3 cols 105_desc
    END
    PATCH_IF (~%stat%~ STRING_EQUAL_CASE ~106~) BEGIN
      READ_2DA_ENTRY row 0 cols 106_label
      READ_2DA_ENTRY row 2 cols 106_name
      READ_2DA_ENTRY row 3 cols 106_desc
    END
    PATCH_IF (~%stat%~ STRING_EQUAL_CASE ~107~) BEGIN
      READ_2DA_ENTRY row 0 cols 107_label
      READ_2DA_ENTRY row 2 cols 107_name
      READ_2DA_ENTRY row 3 cols 107_desc
    END
  END
 BUT_ONLY

 COPY_EXISTING ~weapprof.2da~ ~override~
  COUNT_2DA_COLS cols
  COUNT_2DA_ROWS cols rows
  FOR (row = 7; row < rows; ++row) BEGIN
    READ_2DA_ENTRY row 1 cols stat
    PATCH_IF (~%stat%~ STRING_EQUAL_CASE ~89~) BEGIN
      SET_2DA_ENTRY row 0 cols 107_label
      SET_2DA_ENTRY row 2 cols 107_name
      SET_2DA_ENTRY row 3 cols 107_desc
    END
    PATCH_IF (~%stat%~ STRING_EQUAL_CASE ~90~) BEGIN
      SET_2DA_ENTRY row 0 cols 106_label
      SET_2DA_ENTRY row 2 cols 106_name
      SET_2DA_ENTRY row 3 cols 106_desc
    END
    PATCH_IF (~%stat%~ STRING_EQUAL_CASE ~91~) BEGIN
      SET_2DA_ENTRY row 0 cols 105_label
      SET_2DA_ENTRY row 2 cols 105_name
      SET_2DA_ENTRY row 3 cols 105_desc
    END
    PATCH_IF (~%stat%~ STRING_EQUAL_CASE ~92~) BEGIN
      SET_2DA_ENTRY row 0 cols 104_label
      SET_2DA_ENTRY row 2 cols 104_name
      SET_2DA_ENTRY row 3 cols 104_desc
    END
    PATCH_IF (~%stat%~ STRING_EQUAL_CASE ~104~) BEGIN
      SET_2DA_ENTRY row 0 cols ~_____~
      SET_2DA_ENTRY row 2 cols ~0~
      SET_2DA_ENTRY row 3 cols ~0~
    END
    PATCH_IF (~%stat%~ STRING_EQUAL_CASE ~105~) BEGIN
      SET_2DA_ENTRY row 0 cols ~_____~
      SET_2DA_ENTRY row 2 cols ~0~
      SET_2DA_ENTRY row 3 cols ~0~
    END
    PATCH_IF (~%stat%~ STRING_EQUAL_CASE ~106~) BEGIN
      SET_2DA_ENTRY row 0 cols ~_____~
      SET_2DA_ENTRY row 2 cols ~0~
      SET_2DA_ENTRY row 3 cols ~0~
    END
    PATCH_IF (~%stat%~ STRING_EQUAL_CASE ~107~) BEGIN
      SET_2DA_ENTRY row 0 cols ~_____~
      SET_2DA_ENTRY row 2 cols ~0~
      SET_2DA_ENTRY row 3 cols ~0~
    END
  END
 BUT_ONLY

 COPY_EXISTING ~weapprof.2da~ ~override~
  COUNT_2DA_COLS cols
  FOR (col = 4; col < cols; col += 1) BEGIN
    READ_2DA_ENTRY 8 col cols 89_entry
    READ_2DA_ENTRY 9 col cols 90_entry
    READ_2DA_ENTRY 10 col cols 91_entry
    READ_2DA_ENTRY 11 col cols 92_entry
    READ_2DA_ENTRY 24 col cols 104_entry
    READ_2DA_ENTRY 25 col cols 105_entry
    READ_2DA_ENTRY 26 col cols 106_entry
    READ_2DA_ENTRY 27 col cols 107_entry
    PATCH_IF (VARIABLE_IS_SET %89_entry%) AND (VARIABLE_IS_SET %107_entry%) BEGIN
      SET_2DA_ENTRY 8 col cols %107_entry%
      SET_2DA_ENTRY 27 col cols 0
	END
    PATCH_IF (VARIABLE_IS_SET %90_entry%) AND (VARIABLE_IS_SET %106_entry%) BEGIN
      SET_2DA_ENTRY 9 col cols %106_entry%
      SET_2DA_ENTRY 26 col cols 0
	END
    PATCH_IF (VARIABLE_IS_SET %91_entry%) AND (VARIABLE_IS_SET %105_entry%) BEGIN
      SET_2DA_ENTRY 10 col cols %105_entry%
      SET_2DA_ENTRY 25 col cols 0
	END
    PATCH_IF (VARIABLE_IS_SET %92_entry%) AND (VARIABLE_IS_SET %104_entry%) BEGIN
      SET_2DA_ENTRY 11 col cols %104_entry%
      SET_2DA_ENTRY 24 col cols 0
	END
  END
